<Window x:Class="TrueFriendsApp.View.Windows.AuthorizationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Height="480" Width="450"
        mc:Ignorable="d" Loaded="Window_Loaded"
        Title="AuthorizationWindow" Icon="../../images/icon.ico" ResizeMode="CanMinimize">
    <Window.Resources>
        <FrameworkElement x:Key="MouseCursor" Cursor="../../images/arrow_bold.cur"/>
    </Window.Resources>
    <Grid>
        <Frame 
            Grid.Row="1" Grid.Column="0" NavigationUIVisibility="Hidden"
            x:Name="OutputView" Content="{Binding CurrentPage}">
        </Frame>
    </Grid>
</Window>
